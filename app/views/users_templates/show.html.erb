
<h1><%= @current_user.username%></h1>
<dl>
<dt>ID</dt>
<dd><%= @current_user.id%></dd>
<dt>Edit User</dt>
<dd>Form goes here</dd>
</dl>
<h2>Own photos (<%= @current_user.own_photos.count%>)</h2>
<table border="1">
  <tr>
  <th>Image</th>
  <th>Owner</th>
  <th>Caption</th>
  <th>Posted</th>
  <th></th>
  </tr>
  <% @current_user.own_photos.each do |a_photo| %>
  <tr>
  <td><img src="<%= a_photo.image%>"></td>
  <td><%= a_photo.poster.username%></td>
  <td><%= a_photo.caption%></td>
  <td><%= a_photo.created_at %></td>
  <td><a href="a_photo">Show Details</td>
  </tr>
  <% end %>
</table>
